    "chalk": "^4.1.0",
    "execa": "^4.0.3",
    "husky": "5.1.3",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
